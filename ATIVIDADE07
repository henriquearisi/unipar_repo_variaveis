public class ControleEstoque {
    public static void main(String[] args) {
     
        String nomeProduto;
        double precoProduto;
        int quantidadeProduto;
        
        nomeProduto = "12345";
        precoProduto = 29.99;
        quantidadeProduto = 10;
       
        String marcaProduto;
        marcaProduto = "AlgumaMarca";

        String nomeProduto1 = "Fone de Ouvido";
        double precoProduto1 = 29.99;
        
        System.out.println("Dados do Produto:");
        System.out.println("Nome: " + nomeProduto1);
        System.out.println("Marca: " + marcaProduto);
        System.out.println("Pre√ßo: $" + precoProduto1);
        System.out.println("Quantidade em estoque: " + quantidadeProduto);
    }
}
